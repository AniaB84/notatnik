    "import pandas as pd\n",
    "pd.__version__\n",
    "import numpy as np\n",
    "np.__version__\n",
    "\n",
    "def generator_od_1_do_N(N):\n",
    "    for i in range(1, N+1):\n",
    "        yield i\n",
    "\n",
    "N = 40\n",
    "suma = sum(generator_od_1_do_N(N))\n",
    "\n",
    "print(\"Suma liczb od 1 do\", N, \"to:\", suma)"
